{
    "manifest_version":  3,
    "name":  "speedmod",
    "description":  "Ajuste a velocidade de videos e audios com atalhos simples enquanto a reprodução acontece.",
    "version":  "1.1.1",
    "permissions":  [
                        "storage",
                        "tabs"
                    ],
    "background":  {
                       "service_worker":  "background.js"
                   },
    "action":  {
                   "default_popup":  "popup.html",
                   "default_title":  "speedmod",
                   "default_icon":  {
                                        "16":  "icon.png",
                                        "128":  "icon.png",
                                        "48":  "icon.png",
                                        "32":  "icon.png"
                                    }
               },
    "options_page":  "options.html",
    "content_scripts":  [
                            {
                                "matches":  [
                                                "\u003call_urls\u003e"
                                            ],
                                "js":  [
                                           "content.js"
                                       ],
                                "run_at":  "document_idle",
                                "all_frames":  true
                            }
                        ],
    "icons":  {
                  "16":  "icon.png",
                  "128":  "icon.png",
                  "48":  "icon.png",
                  "32":  "icon.png"
              }
}

